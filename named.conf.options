options {
        directory "/var/cache/bind";
        forwarders {
                1.1.1.1;
                1.0.0.1;
                8.8.8.8;
        };
        dnssec-validation auto;
        listen-on-v6 { any; };
        allow-query { any; };
        querylog yes;
        allow-transfer { any; };
};
